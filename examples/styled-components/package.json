    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "emotion": "8.x",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-emotion": "^9.2.12",
    "react-hot-loader": "^4.11.1",
    "react-spring": "^8.0.25",
    "styled-components": "^4.0.3"
